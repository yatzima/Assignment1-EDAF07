
public class Reversi {

	public static void main(String[] args) {
		Model game = new Model();
		View view = new View();
		Controller con = new Controller(game, view);
		
		con.getBoard();
		con.setDisk(1, 1, 1);
		con.getBoard();
		con.setDisk(5, 4, 2);
		con.getBoard();
		con.setDisk(5, 6, 1);
		con.getBoard();
		
	}

}
