public class Controller {
	private Model board;
	private View view;

	public Controller(Model board, View view) {
		board = this.board;
		view = this.view;
	}

	public void getBoard() {
		view.getBoard(board);
	}

	public void setDisk(int row, int col, int player) {
		board.setDisk(row, col, player);
	}

}
